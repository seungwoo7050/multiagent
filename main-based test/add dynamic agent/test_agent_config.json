{
    "name": "test_calculator_agent",
    "description": "A test agent that can perform calculations",
    "version": "1.0.0",
    "agent_type": "mcp_executor",
    "model": "gpt-4",
    "capabilities": ["TOOL_USE", "REASONING"],
    "parameters": {
      "temperature": 0.2,
      "max_tokens": 800
    },
    "max_retries": 3,
    "timeout": 15000,
    "allowed_tools": ["calculator"],
    "memory_keys": ["context", "previous_calculations"],
    "metadata": {
      "created_by": "test_suite",
      "purpose": "testing"
    },
    "mcp_enabled": true,
    "mcp_context_types": ["AgentInputContext", "AgentOutputContext"]
  }